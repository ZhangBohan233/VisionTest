<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="dvaTest.gui.widgets.CustomAlert"
      prefHeight="100.0" prefWidth="240.0" spacing="10.0" alignment="CENTER_LEFT">

    <padding>
        <Insets topRightBottomLeft="10.0"/>
    </padding>

    <Label fx:id="contentLabel"/>

    <Separator/>

    <HBox alignment="CENTER_RIGHT" spacing="10.0">
        <Button text="%confirm" onAction="#onOkAction"/>
        <Button fx:id="cancelButton" text="%cancel" onAction="#onCancelAction" managed="false"/>
    </HBox>
</VBox>
